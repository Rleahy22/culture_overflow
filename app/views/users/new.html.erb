<%= form_for :user, url: {action: "create"}, html: {class: "signup"} do |f| %>
    <%= f.label :username, "Username" %>
    <%= f.text_field :username %>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>
    <%= f.submit "Join the Party!" %>
<% end %>
